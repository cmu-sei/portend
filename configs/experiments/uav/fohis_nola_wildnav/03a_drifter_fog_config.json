{
  "dataset": {
    "dataset_file": "./input/uav/nola/dataset.json",
    "dataset_class": "portend.datasets.image_dataset.ImageDataSet"
  },
  "output":
  {
    "output_dataset_file": "./output/uav/nola/drifted_images/drifted_images.json"
  },
  "drift_scenario":
  {
    "condition": "fog2",
    "module": "portend.drifts.image.image_drift",
    "params":
    {
      "submodule": "fohis.fohis",
      "img_output_dir": "./output/uav/nola/drifted_images",
      "vra": 100
    }
  }
}
